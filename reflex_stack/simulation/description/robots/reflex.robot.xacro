<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="reflex">

    <xacro:include filename="$(find wrist_controller)/urdf/wrist.urdf.xacro"/>

    <xacro:property name="urdf_path" value="$(find description)/urdf"/>
    <xacro:include filename="${urdf_path}/reflex/reflex.urdf.xacro"/>
    <xacro:include filename="${urdf_path}/reflex/reflex.gazebo.xacro"/>
</robot>
