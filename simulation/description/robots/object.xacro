<?xml version="1.0" ?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="object">

    <!-- currently supports "sphere" and "box" -->
    <xacro:arg name="object_name" default="sphere" />

    <xacro:include filename="$(find description)/urdf/environment/$(arg object_name).urdf.xacro"/>

</robot>
